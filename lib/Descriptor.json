{
	"openapi": "3.0.2",
	"info": {
		"title": "node-pluginsmanager-plugin-commandline",
		"version": "1.0.0",
		"description": "A plugin package to inherit for node-pluginsmanager",
		"contact": {
			"name": "SÃ©bastien VIDAL",
			"url": "https://github.com/Psychopoulet/node-pluginsmanager-plugin-commandline/issues"
		},
		"license": {
			"name": "ISC",
			"url": "https://en.wikipedia.org/wiki/ISC_license"
		}
	},
	"paths": {
		"/node-pluginsmanager-plugin-commandline/descriptor": {
			"get": {
				"description": "",
				"summary": "Return parsed descriptor",
				"responses": {
					"200": {
						"description": "Returned descriptor",
						"content": {
							"application/json": {
								"schema": {
									"type": "object"
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		},
		"/node-pluginsmanager-plugin-commandline/api/commandlines": {
			"get": {
				"description": "",
				"summary": "Return all running command lines",
				"operationId": "getAll",
				"responses": {
					"200": {
						"description": "Returned data",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Commandlines"
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			},
			"put": {
				"description": "",
				"summary": "Create a new command line",
				"requestBody": {
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"command": {
										"description": " Command line's arguments",
										"required": true,
										"schema": {
											"type": "string"
										}
									},
									"arguments": {
										"description": " Command line's arguments",
										"schema": {
											"type": "array",
											"items": {
												"type": "string"
											}
										}
									},
									"options": {
										"description": "Command line's options",
										"schema": {
											"$ref": "#/components/schemas/Options"
										}
									}
								}
							}
						}
					}
				},
				"operationId": "create",
				"responses": {
					"201": {
						"description": "Returned data",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Commandline"
								}
							}
						}
					},
					"default": {
						"description": "An error occured",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Error"
								}
							}
						}
					}
				}
			}
		}
	},
	"components": {
		"schemas": {
			"Commandlines": {
				"type": "array",
				"items": {
					"$ref": "#/components/schemas/Commandline"
				}
			},
			"Commandline": {
				"type": "object",
				"properties": {
					"number": {
						"type": "integer"
					},
					"command": {
						"type": "string"
					},
					"arguments": {
						"type": "array",
						"items": {
							"type": "string"
						}
					},
					"options": {
						"$ref": "#/components/schemas/Options"
					}
				},
				"required": [
					"number",
					"command",
					"arguments",
					"options"
				]
			},
			"Options": {
				"type": "object",
				"properties": {
					"cwd": {
						"type": "string"
					},
					"detached": {
						"type": "boolean"
					},
					"uid": {
						"type": "integer"
					},
					"gid": {
						"type": "integer"
					},
					"windowsHide": {
						"type": "boolean"
					}
				}
			},
			"Error": {
				"type": "object",
				"required": [
					"code",
					"message"
				],
				"properties": {
					"code": {
						"type": "string"
					},
					"message": {
						"type": "string"
					}
				}
			}
		}
	},
	"servers": [
		{
			"url": "http://127.0.0.1:3000",
			"description": "Test server for HTTP requests"
		},
		{
			"url": "ws://127.0.0.1:3001",
			"description": "Test server for socket requests"
		}
	],
	"externalDocs": {
		"description": "Find out more about this API",
		"url": "https://github.com/Psychopoulet/node-pluginsmanager-plugin-commandline#readme"
	}
}